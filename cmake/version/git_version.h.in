#pragma once

namespace GitVersion
{
const char* GIT_TAG="@GIT_TAG@";
const char* GIT_COMMIT="@GIT_COMMIT@@GIT_DIFF@";
const char* GIT_BRANCH="@GIT_BRANCH@";
const char* GIT_DATE="@GIT_DATE@";
const char* BUILD_TIME="@BUILD_TIME@";
const char* BUILD_HOSTNAME="@BUILD_HOSTNAME@";

static const char* get_git_version(void)
{
    return GIT_TAG;
}

static const char* get_git_commit(void)
{
    return GIT_COMMIT;
}

static const char* get_git_branch(void)
{
    return GIT_BRANCH;
}

static const char* get_git_date(void)
{
    return GIT_DATE;
}

static const char* get_build_time(void)
{
    return BUILD_TIME;
}

static const char* get_build_hostname(void)
{
    return BUILD_HOSTNAME;
}
}


